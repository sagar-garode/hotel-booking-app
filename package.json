{
  "name": "fullstack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "workspaces" : ["frontend", "backend"],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:backend": "cd backend && npm run lint",
    "lint:all": "npm run lint:frontend && npm run lint:backend",
    "format": "prettier --check \"**/*.{ts,tsx,js,jsx}\"",
    "format:fix": "prettier --write \"**/*.{ts,tsx,js,jsx}\"",
    "start": "concurrently \"npm run dev --workspace backend\" \"npm start --workspace frontend\""
  },
  " keywords": [],
